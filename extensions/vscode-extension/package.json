{
  "name": "coaxcraft-platform",
  "displayName": "CoaxCraft Platform",
  "description": "Risk assessment and policy enforcement for code repositories",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:coaxcraft.runPrecommit",
    "onCommand:coaxcraft.showLastReceipt",
    "onCommand:coaxcraft.openZeroUIView"
  ],
  "main": "./out/core/activation.js",
  "contributes": {
    "commands": [
      {
        "command": "coaxcraft.runPrecommit",
        "title": "Coaxcraft: Run Precommit"
      },
      {
        "command": "coaxcraft.showLastReceipt",
        "title": "Coaxcraft: Show Last Receipt"
      },
      {
        "command": "coaxcraft.openZeroUIView",
        "title": "Coaxcraft: Open Zero UI View"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "test": "echo \"use VS Code test runner\""
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  }
}
